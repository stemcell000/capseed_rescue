<td class="text-left"><%= object.id%></td>
<td class="text-left"><%= object.name%></td>
<td class="actions text-right">
<%= smart_listing_item_actions [
	  	{name: :edit,    url: edit_project_path(object), title: "Edit"}]%>
<% unless object.productions.exists? %>
	<%= smart_listing_item_actions [
		{name: :destroy, url: project_path(object), title: "Destroy", :confirmation => "Are you sure you want to delete this?", :if => current_user.role="administrator"}
		]%>
<%end%>
</td>