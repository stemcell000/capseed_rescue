<% unless projects.empty? %>    
	  <table  class="table table-striped table-small">
	    <thead>
		    <th class="col-xs-6 ">
		        <%= sort_link(@q, :name, 'Name') %>
		    </th>
		    <th class="col-xs-3 ">
		        <%= sort_link(@q, :created_at, 'Date') %>
		    </th>
		   	<th class="col-xs-2 ">
		    </th>
	    </thead>
	    <tbody>
		  	<% projects.each do |project| %>
	      		<tr>
					<%= render partial: "projects/list_components/object", locals: {object: project} %>
	        		</tr>
	      	<% end %>
	    </tbody>
	  </table>
<div class="row">
  <div class="col-lg-12 text-">
    <%== pagy_bootstrap_nav(@pagy) %>
    <%== pagy_bootstrap_nav_js(@pagy) %>
  </div>
</div>
<%else%>
<div class="row text-">
  <p>No item to display.</p>
</div>
<%end%>
</div>