<p id="notice"><%= notice %></p>

<h1>Listing Vol Units</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vol_units.each do |vol_unit| %>
      <tr>
        <td><%= link_to 'Show', vol_unit %></td>
        <td><%= link_to 'Edit', edit_vol_unit_path(vol_unit) %></td>
        <td><%= link_to 'Destroy', vol_unit, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vol unit', new_vol_unit_path %>
