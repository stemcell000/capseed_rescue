<% unless virus_productions.empty? %>  
<div class="container-fluid">
	  <table class="table table-striped">
	    <thead>
	    <th class="col-lg-1 center">
			<%= sort_link(@q, :nb, 'Number') %>
	  	</th>
	  	<th class="col-lg-1 center">
			<%= sort_link(@q, :date_of_production , 'Date of prod.') %>
	  	</th>
	  	<th class="col-lg-1 center">
	  		<%= sort_link(@q, :production_name, 'Production') %>
	  	</th>
	   <th class="col-lg-4 center">
	    	<%= sort_link(@q, :clone_batches_nb, 'Plasmids') %>
	    </th>
	    	 <th class="col-lg-3 center">
		<%= sort_link(@q, :dosages_accepted, 'Titer to ATCC (validated)') %>
	  	</th>
	    <th class="col-lg-3 text-left"></th>
	    </thead>
	    <tbody>
		  	<% @virus_productions.each do |virus_production| %>
	      		<tr data-id = "<%= virus_production.id %>">
	          			<%= render partial: "virus_productions/list_components/object", locals: {object: virus_production} %>
	        	</tr>
	        	<%end%>
	    </tbody>
	  </table>
		<div class="row">
		  <div class="col-lg-12 text-center">
		    <%== pagy_bootstrap_nav(@pagy) %>
		    <%== pagy_bootstrap_nav_js(@pagy) %>
		  </div>
		</div>
<%else%>
<div class="row text-center">
  <p>No item to display.</p>
</div>
<%end%>
</div>
