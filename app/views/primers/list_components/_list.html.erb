	<% unless primers.empty? %>  
	  <table  class="table table-striped table-small">
	    <thead>
		    <th class="col-lg-2">
		        <%= sort_link(@q, :name, 'Name') %>
		    </th>
		    <th class="col-lg-4">
		        <%= sort_link(@q, :sequence, 'Sequence') %>
		    </th>
		    <th class="col-lg-2 ">
		        <%= sort_link(@q, :available, 'Availability') %>
		    </th>
		    <th class="col-lg-3 ">
		        <%= sort_link(@q, :comment, 'Comment') %>
		    </th>
		  	<th class="col-lg-1 "></th>
	    </thead>
	    <tbody>
		  	<% primers.each do |primer| %>
	      		<tr>
					<%= render partial: "primers/list_components/object", locals: {object: primer} %>
	        	</tr>
	      	<% end %>
	    </tbody>
	  </table>
<div class="row">
  <div class="col-lg-12 text-">
    <%== pagy_bootstrap_nav(@pagy) %>
    <%== pagy_bootstrap_nav_js(@pagy) %>
  </div>
</div>
<%else%>
<div class="row text-">
  <p>No item to display.</p>
</div>
<%end%>
