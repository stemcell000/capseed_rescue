			<div class="panel panel-default">
				<div class="panel-heading">
					<strong><%= "Virus batch".pluralize(@virus_batches.length) %></strong>
				</div>
			<div class="panel-body" >
				<div class="row" >
					<div class="col-lg-12">
						<table>
							<% @arr.each do |elt|%>
								<tr>
									<% elt.each do |b|%>
									<td>
										<div class="col-lg-12 col-md-12 col-sm-12 draggableContainer center">
											<a tabindex="0" data-toggle="popover" data-trigger="focus" data-html = "true"
												title='
													<div class="row"> 
														<div class="col-lg-5 text-left">
															<strong><%=b.name%></strong>
														</div>
														<div class="col-lg-12 right">
															<%unless b.trash%>
																<%= link_to "Edit", edit_from_inventory_virus_production_virus_batch_path(b.virus_production, b), :class =>"btn btn-primary btn-xs font80pc", :remote => true %>
																<%= link_to "Remove", destroy_from_inventory_virus_production_virus_batch_path(b.virus_production, b.id), :class =>"btn btn-danger btn-xs font80pc", :title=>"Remove from list.", :remote => false %>
															<%else%>
																<%= link_to "Rescue", destroy_from_inventory_virus_production_virus_batch_path(b.virus_production, b.id), :class =>"btn btn-warning btn-xs font80pc", :title=>"Rescue", :remote => false %>
															<%end%>	
														</div>	
													</div>'
												data-content='
													<div class="col-lg-12 text-left">
														<small>
														<strong>Barcode : </strong><%= b.barcode.nil? ? "-" : b.barcode %><br />
														<strong>Date of production : </strong><%= b.virus_production.date_of_production.nil? ?  "-" : b.virus_production.date_of_production.strftime("%h %e, %Y ") %><br />
														<strong>Date of thawing : </strong><%= b.date_of_thawing.nil? ?  "-" : b.date_of_thawing.strftime("%h %e, %Y ") %><br />
														<strong>Volume : </strong><%= b.volume.nil? ? "-" : b.volume %> <%= b.vol_unit_id.blank? ? "" : b.vol_unit.name %><br />
														<strong>Box : </strong><%= b.position.nil? ? "-" : b.position.box.name %> <br />
														<strong>Slot : </strong><%= b.position.nil? ? "-" : b.position.name.upcase() %> <br />
														<% if !b.comment.blank? %>
															<strong>Comments : </strong><br /><%= b.comment || "-" %><br />
														<%end%>
														</small>
													</div>
												'
											>
													<div class="center">
														<img src= <%=b.set_tube_status %> alt="not-empty" class=<%=b.set_draggable%> data-id="<%=b.id %>"/>
														<div class="<%= b.set_color_from_position%>"><strong><small><%= b.name %></small></strong></div>	
													</div>
											</a>	
											</div>
									</td>
								<%end%>
								</tr>
								<%end%>
							</table>
						</div><!--End of col-lg-3-->
					</div><!--End pf row-->
				</div><!--End of panel body-->
			</div><!--End of panel-->

