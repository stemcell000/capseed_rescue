 <% unless plasmid_boxes.empty? %>  
 <div class="container-fluid">

	  <table class="table table-striped">
	    <thead>
	      
	    <th class="col-lg-5 col-md-5 col-sm-5">
	        <%= sort_link(@q, :name, 'Name') %>
	    </th>
	    	 <th class="col-lg-4 col-md-4 col-sm-4">
	        <%= sort_link(@q, :barcode, 'Barcode') %>
	    </th>
	    <th class="col-lg-3 col-md-3 col-sm-3 center"></th>
	    
	    </thead>
	    <tbody>
		  	<% plasmid_boxes.uniq.each do |plasmid_box| %>
	      		<tr>
	          		<%= render partial: "plasmid_boxes/list_components/object", locals: {object: plasmid_box} %>
	        	</tr>
	      	<% end %>
	    </tbody>
	  </table>
<div class="row">
		  <div class="col-lg-12 text-center">
		    <%== pagy_bootstrap_nav(@pagy) %>
		    <%== pagy_bootstrap_nav_js(@pagy) %>
		  </div>
	</div>
	<%else%>
<div class="row text-center">
  <p>No item to display.</p>
</div>
<%end%>
</div>
