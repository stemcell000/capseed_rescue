<% unless @virus_batches.empty?%>
<table>
	<thead>
        <tr>
            <th><%= sort_link(@q, :name) %></th>
            <th><%= sort_link(@q, :barcode) %></th>
        </tr>
    </thead>
    <tbody>
    		<%@virus_batches.each do |virus_batch|%>
    		<tr>
    			<td>
    				<%=virus_batch.name%>
    			</td>
    			 <td>
    				<%=virus_batch.barcode%>
    			</td>
		</tr>
		<%end%>
    </tbody>
</table>
<div class="row">
  <div class="col-lg-12 text-center">
    <%== pagy_bootstrap_nav(@pagy) %>
    <%== pagy_bootstrap_nav_js(@pagy) %>
  </div>
</div>
<%else%>
	<span>No sample to display</span>
<%end%>
